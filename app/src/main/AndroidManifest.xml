<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chef.app.demo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".View.Activities.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Activities.HomeActivity"
            android:label="Dashboard"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".View.Activities.PickUpActivity"
            android:label="Pick Up"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".View.Activities.DeliveryActivity"
            android:label="Delivery"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.Activities.PickUpDetailsActivity"
            android:label="Pick Up Details" />
        <activity
            android:name=".View.Activities.DeliveryDetailsActivity"
            android:label="Delivery Details" />
        <activity
            android:name=".View.Activities.ExchangeDetailsActivity"
            android:screenOrientation="portrait"
            android:label="Exchange Details" />
        <activity android:name=".View.Activities.HistoryActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".View.Activities.LogInActivity"></activity>
    </application>

</manifest>